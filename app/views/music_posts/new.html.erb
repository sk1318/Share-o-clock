<%= form_with(model: @music_post, local: true) do |f| %>
   <div class="field">
    <%= f.text_area :music_name, placeholder: "曲名を入力..."  %>
  </div>
  
  <div class="field">
    <%= f.text_area :content, placeholder: "歌詞を入力..." %>
  </div>
  
  <div class="field">
    <%= f.text_area :youtube, placeholder: "youtube..." %>
  </div>
  <%= f.submit "送信",class: "btn btn-primary" %>
  <% end %>